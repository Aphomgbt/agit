<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android" >
	<ImageView
		android:id="@+id/osv_object_type_icon"
		android:layout_width="24dip"
		android:layout_height="24dip"
		android:src="@drawable/commit_24" />
    <TextView android:id="@+id/osv_object_type_text"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"/>
	<com.madgag.agit.views.ObjectIdView android:id="@+id/osv_object_id_text"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:typeface="monospace" />
	<LinearLayout
		android:id="@+id/osv_type_specific_data_frame"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        />
</merge>